{
    "name": "jamosaur/php-toggl",
    "description": "PHP implementation of the Toggl API",
    "keywords": [
        "Toggl"
    ],
    "homepage": "https://github.com/jaaaaaaaaaam/phpToggl",
    "license": "MIT",
    "authors": [
        {
            "name": "James Wallen-Jones",
            "email": "j.wallen.jones@googlemail.com",
            "homepage": "http://www.jamosaur.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzle/guzzle": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Jamosaur\\Toggl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jamosaur\\Toggl\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
